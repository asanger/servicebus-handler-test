{
  "name": "servicebus-test",
  "version": "0.0.1",
  "description": "Servicebus Handler Test",
  "main": "app.js",
  "scripts": {
    "build": "tsc",
    "lint": "./node_modules/.bin/eslint . --ext .js,.ts --fix",
    "start": "tsc && node ./lib/app.js",
    "start:dev": "export NODE_ENV=development && nodemon -e ts -w ./src -w ./app.ts --exec 'npm start'",
    "test": "tsc && ./node_modules/.bin/mocha ./lib/tests --recursive --exit",
    "test:integration": "./node_modules/.bin/mocha ./lib/tests/integration --recursive --exit"
  },
  "keywords": [],
  "author": "Alec Sanger",
  "directories": {
    "lib": "./lib"
  },
  "license": "",
  "dependencies": {
    "@eventide/message-db": "^1.1.6",
    "axios": "^0.21.1",
    "boom": "^7.3.0",
    "dotenv": "^8.2.0",
    "fastify": "^2.10.0",
    "fastify-boom": "^0.1.0",
    "fastify-caching": "^5.0.0",
    "fastify-compress": "^1.1.0",
    "fastify-cors": "^2.1.3",
    "knex": "^0.20.1",
    "knex-stringcase": "^1.3.0",
    "pg": "^7.12.1",
    "servicebus": "^2.3.3",
    "servicebus-register-handlers": "^0.2.2",
    "ts-node": "^8.4.1",
    "uuid": "^3.3.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "babel-eslint": "^10.0.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "mocha": "^6.2.2",
    "nodemon": "^1.19.4",
    "prettier": "^1.18.2",
    "sinon": "^7.5.0",
    "typescript": "^3.6.4",
    "watch": "^1.0.2"
  },
  "engines": {
    "node": "^8.11.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
